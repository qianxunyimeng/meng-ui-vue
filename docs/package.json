{
  "name": "@meng-ui-vue/docs",
  "version": "0.0.1",
  "scripts": {
    "dev": "pnpm gen-locale && vitepress dev .",
    "build": "cross-env NODE_ENV=production && vitepress build .",
    "serve": "cross-env NODE_ENV=production && vitepress serve . --port 8001",
    "gen-locale": "rimraf .vitepress/i18n && sucrase-node .vitepress/build/crowdin-generate.ts",
    "crowdin-credentials": "sucrase-node .vitepress/build/crowdin-credentials.ts"
  },
  "dependencies": {
    "@docsearch/css": "^3.0.0-alpha.42",
    "@docsearch/js": "^3.0.0-alpha.42",
    "@docsearch/react": "^3.0.0-alpha.42",
    "@vueuse/core": "7.1.2",
    "axios": "0.24.0",
    "clipboard-copy": "4.0.1",
    "element-plus": "npm:element-plus@latest",
    "meng-ui-vue":"0.1.4-beta8",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0"
  },
  "devDependencies": {
    "@crowdin/cli": "3.7.2",
    "@types/markdown-it": "12.2.3",
    "cross-env": "7.0.3",
    "markdown-it": "12.2.0",
    "markdown-it-container": "3.0.0",
    "vite-plugin-inspect": "0.3.11",
    "vitepress": "0.18.1"
  }
}
