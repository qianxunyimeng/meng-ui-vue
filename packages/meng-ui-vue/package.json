{
  "name": "meng-ui-vue",
  "version": "0.1.4-beta6",
  "description": "A Component Library for Vue 3",
  "homepage": "http://qianxun.shop/meng-ui-vue",
  "keywords": [
    "meng-ui",
    "component library",
    "ui framework",
    "ui",
    "vue"
  ],
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./es/index.mjs"
    },
    "./es": "./es/index.mjs",
    "./lib": "./lib/index.js",
    "./*": "./*"
  },
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "style": "dist/index.css",
  "unpkg": "dist/index.full.js",
  "jsdelivr": "dist/index.full.js",
  "sideEffects": [
    "dist/*",
    "theme-chalk/*.css",
    "theme-chalk/src/*.scss",
    "es/components/*/style/*",
    "lib/components/*/style/*",
    "lib/components/**/*",
    "es/components/**/*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qianxunyimeng/meng-ui-vue.git"
  },
  "bugs": {
    "url": "https://github.com/qianxunyimeng/meng-ui-vue/issues"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.10.2",
    "@vueuse/core": "^7.0.0",
    "async-validator": "^4.0.7",
    "dayjs": "^1.10.7",
    "lodash": "^4.17.21",
    "memoize-one": "^6.0.0",
    "normalize-wheel-es": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "*",
    "csstype": "^2.6.0",
    "vue-router": "^4.0.0"
  },
  "vetur": {
    "tags": "tags.json",
    "attributes": "attributes.json"
  },
  "web-types": "web-types.json",
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ]
}
